<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.1</TargetFramework>
    <RootNamespace>SchoolBotAPI</RootNamespace>
    <Version>1.4.1.0</Version>
    <Company>KriseevM</Company>
    <Product>vkschoolbot API client class library</Product>
    <Description>Simple library for working with vkschoolbot API from the client side</Description>
    <RepositoryUrl>https://github.com/KriseevM/vkschoolbot-api-client-lib.git</RepositoryUrl>
    <RepositoryType>Github-repository</RepositoryType>
    <NeutralLanguage>en-US</NeutralLanguage>
    <AssemblyVersion>1.4.1.0</AssemblyVersion>
    <FileVersion>1.4.1.0</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
    <PackageReference Include="Newtonsoft.Json.Schema" Version="3.0.13" />
  </ItemGroup>

</Project>
