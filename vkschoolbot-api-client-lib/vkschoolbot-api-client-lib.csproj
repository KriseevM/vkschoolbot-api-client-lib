<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.1</TargetFramework>
    <RootNamespace>SchoolBotAPI</RootNamespace>
    <Version>0.2.6</Version>
    <Company>KriseevM</Company>
    <Product>vkschoolbot API client class library</Product>
    <Description>Simple library for working with vkschoolbot API from the client side</Description>
